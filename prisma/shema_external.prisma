generator client {
  provider = "prisma-client-js"
  binaryTargets   = ["native", "linux-musl"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
//Создатели
model creators {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Языки
model languages {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Места
model places {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Читатели
model readers {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Авторы
model authors {
  id String @id @default(uuid()) @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
//Составители
model compilers {
  id String @id @default(uuid()) @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
//Редакторы
model editors {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Художники
model painters {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Переводчики
model translators {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Размещение
model placement {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Выдача
model extradition {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Переиздание
model reissue {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Издание
model publication {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Переплеты
model bindings {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Характеры
model characters {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Заглавия
model titles {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Вид издания
model type_publication {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Издательства
model publishing_houses {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
//Аннотации
model annotations {
    id String @id @default(uuid()) @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}