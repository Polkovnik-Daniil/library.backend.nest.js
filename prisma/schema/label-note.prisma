model LabelNote {
    id              String      @id @default(uuid()) @unique
    noteId          String      @unique
    labelId         String      @unique
    isPublic        Boolean     @default(false)

    Notes           Note[]
    Labels          Label[]
    Editor          Editor[]
    Reader          Reader[]
    
    createdAt       DateTime    @default(now())
    updatedAt       DateTime    @updatedAt
}